# Copy to .env and fill in the values.
# The n8n UI will be at http://localhost:5678 (basic auth below)
OPENAI_API_KEY=sk-your-openai-key
QUALIFIRE_API_KEY=qualifire_your_api_key

# n8n local auth (change for your machine)
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=change-me-please

# Any 32+ character random string. Example:
# macOS/Linux:  openssl rand -hex 32
N8N_ENCRYPTION_KEY=replace-with-random-hex

# Local timezone
TZ=Asia/Jerusalem
